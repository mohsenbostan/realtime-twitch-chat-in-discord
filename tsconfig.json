{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "es2022",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "outDir": "./dist",
    "baseUrl": "./"
  }
